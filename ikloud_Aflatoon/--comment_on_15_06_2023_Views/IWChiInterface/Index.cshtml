<br />
<div class="row col-md-12">
    <button type="button" id="Close" value="Close" class="btn-info col-md-1" onclick="Close()">Close</button>
    <button type="button" id="TerminateRequest" value="TerminateRequest" class="btn-info col-md-offset-1 col-md-1" onclick="TerminateRequest()">Terminate</button>
    <button type="button" id="CheckStatus" value="CheckStatus" class="btn-info col-md-offset-1 col-md-1" onclick="CheckStatus()">Check Status</button>
</div>
<br />
<div class="row col-md-12">
    <div id="div1" class="col-md-5" style="background-color:aliceblue;padding:5px;">
        <div class="row col-md-12">
            <label class="label col-md-offset-2 col-md-8" style="font-size:x-large;text-align:center;color:Highlight;"><b>RRF : DBS Inward Return Processing</b></label>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="CreateRRF" class="btn-info col-md-offset-3 col-md-6" onclick="CreateRRF()"> Create RRF </button>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="UploadRRF" class="btn-info col-md-offset-3 col-md-6" onclick="UploadRRF()"> Upload RRF </button>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="LoadResOcrRRF" class="btn-info col-md-offset-3 col-md-6" onclick="LoadResOcrRRF()"> Load RES/OCR </button>
        </div>
        <br />
        <div class="row col-md-12">
            <a id="Show_Records_RRF" class="col-md-offset-3 col-md-3" onclick="Show_Records_RRF()" data-toggle="modal" data-target="#basicModal"> Show Record </a>
            <a id="Show_Files_RRF" class="col-md-offset-1 col-md-3" onclick="Show_Files_RRF()" data-toggle="modal" data-target="#basicModal"> Show Files </a>
        </div>
    </div>
    <div class="col-md-1"></div>
    <div id="div2" class="col-md-5" style="background-color:aliceblue;padding:5px;">
        <div class="row col-md-12">
            <label class="label col-md-offset-2 col-md-8" style="font-size:x-large;text-align:center;color:Highlight;"><b>PXF : DBS Inward Processing</b></label>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="LoadPXF" class="btn-info col-md-offset-3 col-md-6" onclick="LoadPXF()"> Load PXF </button>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="ExtractDataPXF" class="btn-info col-md-offset-3 col-md-6" onclick="ExtractDataPXF()"> Extract Data </button>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="GenerateRMI" class="btn-info col-md-offset-3 col-md-6" onclick="GenerateRMI()"> Generate RMI </button>
        </div>
        <br />
        <div class="row col-md-12">
            <a id="Show_Records_PXF" class="col-md-offset-3 col-md-3" onclick="Show_Records_PXF()" data-toggle="modal" data-target="#basicModal"> Show Record </a>
            <a id="Show_Files_PXF" class="col-md-offset-1 col-md-3" onclick="Show_Files_PXF()" data-toggle="modal" data-target="#basicModal"> Show Files </a>
        </div>
    </div>
</div>

@*<span style="width:30%"></span>*@


<br />
<div class="row col-md-12">
    <div id="div3" class="col-md-5" style="background-color:aliceblue;padding:5px;">
        <div class="row col-md-12">
            <label class="label col-md-offset-2 col-md-8" style="font-size:x-large;text-align:center;color:Highlight;"><b>LVB Inward And Return Processing</b></label>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="GeneratePXF" class="btn-info col-md-offset-3 col-md-6" onclick="GeneratePXF()"> Generate PXF </button>
        </div>
        <br />
        <div class="row col-md-12">
            <a id="Show_Records_LVB_PXF" class="col-md-offset-3 col-md-3" onclick="Show_Records_LVB_PXF()" data-toggle="modal" data-target="#basicModal"> Show Record </a>
            <a id="Show_Files_LVB_PXF" class="col-md-offset-1 col-md-3" onclick="Show_Files_LVB_PXF()" data-toggle="modal" data-target="#basicModal"> Show Files </a>
        </div>
        <br />
        <div class="row col-md-12">
            <button type="button" id="ReadRRF" class="btn-info col-md-offset-3 col-md-6" onclick="ReadRRF()"> Read RRF </button>
        </div>
        <br />
        <div class="row col-md-12">
            <a id="Show_Records_LVB_RRF" class="col-md-offset-3 col-md-3" onclick="Show_Records_LVB_RRF()" data-toggle="modal" data-target="#basicModal"> Show Record </a>
            <a id="Show_Files_LVB_RRF" class="col-md-offset-1 col-md-3" onclick="Show_Files_LVB_RRF()" data-toggle="modal" data-target="#basicModal"> Show Files </a>
        </div>
    </div>
</div>

<div class="modal fade" id="basicModal" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="width:1200px;">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">Display Data</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                @*<h3>Modal Body</h3>*@
                @*@Html.Action("RejectReason", "OWBranchLevelDataEntry")*@
                <div id="tableData">

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                @*<button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div>
    </div>
</div>

@if (ViewBag.Message != null)
{
    <script type="text/javascript">
        var RootUrl = '@Url.Content("~/")';
        alert("@ViewBag.Message");
        //var url = RootUrl + "IWChiInterface/Index";
