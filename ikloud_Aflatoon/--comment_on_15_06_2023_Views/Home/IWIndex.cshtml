@{
    ViewBag.Title = "Home";
    ViewBag.header = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<link href="~/assets/bjqs.css" rel="stylesheet" />
<link href="~/assets/demo.css" rel="stylesheet" />
@*<script src="~/Scripts/jquery-2.1.4.min.js"></script>*@
<script src="~/assets/js/bjqs-1.3.min.js"></script>
<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>

<script type="text/javascript">
    var RootUrl = '@Url.Content("~/")';
</script>

@if (ViewBag.userlogin == false)
{

    <script type="text/javascript">

        var conval = confirm("This user already logged in if you contiue previuos session will expire!!");

        if (conval == false) {
            //alert(username);
            window.location = RootUrl + "Login/Logout?user=" + '@Session["LoginID"].ToString()';
        }
        else {
            //window.location = "/Home/setSession";
            $.ajax({
                url: RootUrl + 'Home/setSession',
                dataType: 'html',
                success: function (data) {
                }
            });
        }

    </script>

}


@if (ViewBag.msg != null)
{
    <script type="text/javascript">
       // alert('Aila!!!');
        //document.getElementById('nodatafound').style.display = 'block';
        alert('@ViewBag.msg');
    </script>
}

@if (ViewBag.IsBRF_MessageDisplay == "Y")
{
    if (ViewBag.Initial == "Y")
    {
        <marquee width="100%" direction="left" height="100px"><b>CTS RETURN SESSION WAS COMPLETED AT @ViewBag.ValMsg</b></marquee>
    }
}


