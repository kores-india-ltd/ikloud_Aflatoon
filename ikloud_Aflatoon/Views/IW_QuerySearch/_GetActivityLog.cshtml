                        <th>Account Modified</th>

                        <th>SQ Maker Status</th>
                        <th>SQ Maker ID</th>

                        <th>SQ Checker Status</th>
                        <th>SQ Checker ID</th>

                        <th>SQ Maker TimeStamp</th>
                        <th>SQ Checker TimeStamp</th>

                    </tr>
                </thead>
                <tbody>
                    @if (Model.Get_IWSQActivityLog != null && Model.Get_IWSQActivityLog.Any())
                    {


                        foreach (var item in Model.Get_IWSQActivityLog)
                        {
                            <tr>
                                <td>@item.SQ_ChequeNo</td>
                                <td>@item.SQ_AL_ChequeNo</td>
                                <td>
                                    @(item.SQ_ChequeNo_Comparison == "Matched" ? "N" :
                                     item.SQ_ChequeNo_Comparison == "Not Matched" ? "Y" : " ")
                                </td>

                                <td>@item.SQ_SortCode</td>
                                <td>@item.SQ_AL_SortCode</td>
                                <td>
                                    @(item.SQ_SortCode_Comparison == "Matched" ? "N" :
                                    item.SQ_SortCode_Comparison == "Not Matched" ? "Y" : " ")
                                </td>

                                @*<td>@(item.SQ_SortCode_Comparison == "Matched" ? "N" : "Y")</td>*@

                                <td>@item.SQ_SAN</td>
                                <td>@item.SQ_AL_SAN</td>
                                <td>
                                    @(item.SQ_SAN_Comparison == "Matched" ? "N" :
                                     item.SQ_SAN_Comparison == "Not Matched" ? "Y" : " ")
                                </td>

                                @*<td>@(item.SQ_SAN_Comparison == "Matched" ? "N" : "Y")</td>*@

                                <td>@item.SQ_TC</td>
                                <td>@item.SQ_AL_TC</td>
                                <td>
                                    @(item.SQ_TC_Comparison == "Matched" ? "N" :
                                     item.SQ_TC_Comparison == "Not Matched" ? "Y" : " ")
                                </td>

                                @*<td>@(item.SQ_TC_Comparison == "Matched" ? "N" : "Y")</td>*@

                                <td>@item.SQ_CreditAccNo</td>
                                <td>@item.SQ_AL_CreditAccNo</td>
                                <td>
                                    @(item.SQ_CreditAccNo_Comparison == "Matched" ? "N" :
                                    item.SQ_CreditAccNo_Comparison == "Not Matched" ? "Y" : " ")
                                </td>

                                @*<td>@(item.SQ_CreditAccNo_Comparison == "Matched" ? "N" : "Y")</td>*@


                                <td>
                                    @(item.SQ_Maker_status=="2" ? "Accept" :
                                    item.SQ_Maker_status=="3" ? "Reject" : " ")
                                </td>

                                @*<td>@(item.SQ_Maker_status == "2" ? "Accept" : "Reject")</td>*@
                                <td>@item.SQ_MakerID</td>



                                <td>
                                    @(item.SQ_Checker_status == "2" ? "Accept" :
                                    item.SQ_Checker_status == "3" ? "Reject" :
                                    item.SQ_Checker_status=="5" ? "Return To Maker": " ")
