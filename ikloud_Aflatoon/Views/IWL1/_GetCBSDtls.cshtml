                </div>


                <div class="w3-container">
                    <label>
                        cpps flag:
                    </label>
                    <label>
                        @if (@Model.cbsdls.Split('|').ElementAt(3) == "3021" || @Model.cbsdls.Split('|').ElementAt(3) == "1021")
                        {<label>
                                Y
                            </label>}
                        else
                        {<label>
                                N
                            </label>}-->
                    @*@Model.cbsdls.Split('|').ElementAt(10)*@
